{
	"name": "EcoSurf",
	"description": "Surf climate friendly!",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://*/*"],
			"js": ["contentScript.js"],
			"run_at": "document_end"
		}
	],
	"action": {
		"default_popup": "popup.html"
	},
	"permissions": ["storage", "contentSettings", "activeTab", "scripting"]
}